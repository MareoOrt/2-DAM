<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".MainActivity">

        <Button
            android:id="@+id/bt_alerta"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Alerta Normal" />

        <Button
            android:id="@+id/bt_sel_mult"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Seleccion Multiple" />

        <Button
            android:id="@+id/bt_alert_basic"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Alerta Mas Basica" />
        <Button
            android:id="@+id/btDialogPersonal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Alerta Personalizada" />

        <ImageView
            android:id="@+id/iv_Imagen"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
    </LinearLayout>
</layout>